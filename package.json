{
  "name": "llmlayer",
  "version": "0.2.0",
  "description": "Official JavaScript / TypeScript SDK for the LLMLayer Search & Answer API",
  "type": "module",
  "engines": {
   "node": ">=18.17"
  },
  "author": {
    "name": "Yassine Khazzan",
    "email": "yassine@llmlayer.ai",
    "url": "https://llmlayer.ai"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/YassKhazzan/llmlayer_js"
  },
  "bugs": {
    "url": "https://github.com/YassKhazzan/llmlayer_js/issues"
  },
  "homepage": "https://github.com/YassKhazzan/llmlayer_js#readme",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/cjs/index.js"
  },
  "main": "./dist/cjs/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.json && tsc -p tsconfig.cjs.json && cp cjs-package.json dist/cjs/package.json",
    "dev": "ts-node-dev --respawn src/examples/local-demo.mts"
  },
  "dependencies": {
    "undici": "^6.21.3"
  },
  "devDependencies": {
    "dotenv": "^10.0.0",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
